            KeyCode::SPACE,
            ModifierFlag::COMMAND_R,
            KeyCode::DELETE
        </autogen>
    </item>
    
    <item>
        <name>Command R then L into power delete</name>
        <identifier>private.double_command_to_power_deleate</identifier>
        <autogen>
            __KeyToKey__ 
            KeyCode::COMMAND_L,
            ModifierFlag::COMMAND_R,
            KeyCode::DELETE,
            ModifierFlag::OPTION_R,
        </autogen>
    </item>

    <item>
        <name>Remap J K L ; ' \ into ( ) _ = - + with command left</name>
        <identifier>private.remap_top_row_specials</identifier>
        <autogen>
            __KeyToKey__ 
            KeyCode::J, 
            ModifierFlag::COMMAND_L, 
            KeyCode::KEY_9, ModifierFlag::SHIFT_L
        </autogen>
         <autogen>
            __KeyToKey__ 
            KeyCode::K, 
            ModifierFlag::COMMAND_L, 
            KeyCode::KEY_0, ModifierFlag::SHIFT_L
        </autogen>
        <autogen>
            __KeyToKey__ 
            KeyCode::L, 
            ModifierFlag::COMMAND_L, ModifierFlag::SHIFT_L,
            KeyCode::MINUS,
        </autogen>        
        <autogen>
            __KeyToKey__ 
            KeyCode::L,
            ModifierFlag::COMMAND_L,
            KeyCode::MINUS, ModifierFlag::SHIFT_L
        </autogen>
         <autogen>
            __KeyToKey__ 
            KeyCode::SEMICOLON, 
            ModifierFlag::COMMAND_L, 
            KeyCode::EQUAL
        </autogen>
        <autogen>
            __KeyToKey__ 
            KeyCode::QUOTE, 
            ModifierFlag::COMMAND_L,
            KeyCode::MINUS
        </autogen>
        <autogen>
            __KeyToKey__ 
            KeyCode::BACKSLASH, 
            ModifierFlag::COMMAND_L,
            KeyCode::EQUAL, ModifierFlag::SHIFT_L
        </autogen>
    </item>
    
    <item>
