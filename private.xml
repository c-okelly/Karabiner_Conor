<?xml version="1.0"?>
<root>
    <item><name>Chrome settings</name>
        <item>
            <name>Enable table shifting with Contorl J and control H</name>
            <identifier>chorme_tab_shifiting</identifier>

            <only>GOOGLE_CHROME</only>

            <autogen>
                __KeyToKey__
                KeyCode::K,
                ModifierFlag::CONTROL_L,
                KeyCode::TAB,
                ModifierFlag::CONTROL_L,
            </autogen>

            <autogen>
                __KeyToKey__
                KeyCode::J,
                ModifierFlag::CONTROL_L,
                KeyCode::TAB,
                ModifierFlag::CONTROL_L | ModifierFlag::SHIFT_L
            </autogen>
 
        </item>
        <item>
        <name>change windows with Control L</name>
             <identifier>change_chrome_window</identifier>
                <autogen>
                __KeyToKey__
                KeyCode::L,
                ModifierFlag::CONTROL_L,
                KeyCode::BACKQUOTE,
                ModifierFlag::COMMAND_R
            </autogen>
        </item>

        <item>
            <name>Back one page with Control H => Chrome</name>
            <identifier>chrome_back_history</identifier>

            <only>GOOGLE_CHROME</only>

            <autogen>
                __KeyToKey__
                KeyCode::H,
                ModifierFlag::CONTROL_L,
                KeyCode::BRACKET_LEFT,
                ModifierFlag::COMMAND_L,
            </autogen>
        </item>
    </item>
    
    <item><name>Terminal settings</name>
        <name>Shift left and right in terminal wiht command L left and right arrows</name>
        <identifier>terminal_shifiting</identifier>
        <only>TERMINAL</only>
    
        <autogen>
                __KeyToKey__
                KeyCode::L,
                ModifierFlag::COMMAND_L | ModifierFlag::OPTION_L,
                KeyCode::CURSOR_RIGHT,
                ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L
        </autogen>
        <autogen>
                __KeyToKey__
                KeyCode::J,
                ModifierFlag::COMMAND_L | ModifierFlag::OPTION_L,
                KeyCode::CURSOR_LEFT,
                ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L
        </autogen>
    </item>

    <item>
        <name>Open spotlight with Control space</name>
        <identifier>Open_spotlight</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::SPACE,
            ModifierFlag::CONTROL_L,
            KeyCode::SPACE,
            ModifierFlag::COMMAND_R
        </autogen>
    </item>
    
    <item>
        <name>Open spotlight with finder search with Control F </name>
        <identifier>Open_finder_search</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::F,
            ModifierFlag::CONTROL_L,
            KeyCode::SPACE,
            ModifierFlag::COMMAND_R | ModifierFlag::OPTION_R
        </autogen>
    </item>  
    <item>
        <name>Danish doller key now BACKQUOTE</name>
        <identifier>private.change_danish_doller_to_BACKQUOTE</identifier>
        <autogen>
            __KeyToKey__ 
            KeyCode::DANISH_DOLLAR,
            KeyCode::BACKQUOTE
        </autogen>
    </item>
    <item>
        <name>Disable command left / tab to change app => create typing conflict</name>
        <identifier>private.change_command_L_Tab</identifier>
        <autogen>
            __KeyToKey__ 
            KeyCode::TAB,
            ModifierFlag::COMMAND_L,
            KeyCode::TAB
        </autogen>
    </item>
    <item>
        <name>Backquote key  now control key</name>
        <identifier>private.change_shift</identifier>
        <autogen>
            __KeyToKey__ 
            KeyCode::BACKQUOTE,
            KeyCode::CONTROL_L
        </autogen>
    </item>
    
    <item>
        <name>Command_R and Option_L outputs Option_R</name>
        <identifier>private.option_l_and_command_r</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::OPTION_L, 
            ModifierFlag::COMMAND_R,
            KeyCode::OPTION_R
        </autogen>
    </item>
    
    <item>
        <name>Home Row Arrow Keys: Option + JIKL to ^&lt;&gt;v</name>
        <identifier>private.option_homerowarrows</identifier>
