            KeyCode::H,
            ModifierFlag::OPTION_L,
            KeyCode::KEY_1, ModifierFlag::SHIFT_L,
        </autogen>
        <autogen>
            __KeyToKey__ 
            KeyCode::Y,
            ModifierFlag::OPTION_L,
            KeyCode::KEY_4, ModifierFlag::SHIFT_L,
        </autogen>
        <autogen>
            __KeyToKey__ 
            KeyCode::U,
            ModifierFlag::OPTION_L,
            KeyCode::KEY_2, ModifierFlag::SHIFT_L,
        </autogen>
        <autogen>
            __KeyToKey__ 
            KeyCode::O,
            ModifierFlag::OPTION_L,
            KeyCode::KEY_3, ModifierFlag::SHIFT_L,
        </autogen>
        <autogen>
            __KeyToKey__ 
            KeyCode::SEMICOLON,
            ModifierFlag::OPTION_L,
            KeyCode::KEY_5, ModifierFlag::SHIFT_L,
        </autogen>
        <autogen>
            __KeyToKey__ 
            KeyCode::QUOTE,
            ModifierFlag::OPTION_L,
            KeyCode::KEY_8, ModifierFlag::SHIFT_L,
        </autogen>
        <autogen>
            __KeyToKey__ 
            KeyCode::N,
            ModifierFlag::OPTION_L,
            KeyCode::KEY_7, ModifierFlag::SHIFT_L,
        </autogen>
        <autogen>
            __KeyToKey__ 
            KeyCode::M,
            ModifierFlag::OPTION_L,
            KeyCode::KEY_6, ModifierFlag::SHIFT_L,
        </autogen>
    </item>
    
